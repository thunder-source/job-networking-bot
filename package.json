{
  "name": "job-networking-bot",
  "version": "1.0.0",
  "description": "A CLI tool for job networking automation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "cold-email-bot": "./dist/src/simple-cli.js"
  },
  "scripts": {
    "start": "node dist/src/simple-cli.js",
    "cli": "node dist/src/simple-cli.js",
    "full-cli": "node dist/src/full-cli.js",
    "dev": "nodemon",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "linkedin:example": "ts-node examples/linkedin-example.ts",
    "ai:example": "ts-node examples/ai-example.ts",
    "template:example": "ts-node examples/template-example.ts",
    "email:example": "ts-node examples/email-example.ts",
    "email-lookup:example": "ts-node examples/email-lookup-example.ts",
    "scheduler:example": "ts-node examples/scheduler-example.ts",
    "rate-limiter:example": "ts-node examples/rate-limiter-example.ts",
    "safety:example": "ts-node examples/safety-example.ts"
  },
  "keywords": [
    "cli",
    "job",
    "networking",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/node-cron": "^3.0.11",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.12",
    "mongoose": "^8.19.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.8",
    "openai": "^4.104.0",
    "ora": "^7.0.1",
    "playwright": "^1.56.0",
    "pm2": "^6.0.13",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.7.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
